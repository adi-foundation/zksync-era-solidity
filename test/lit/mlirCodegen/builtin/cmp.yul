// RUN: solc --yul --yul-dialect=evm --mlir-action=print-init --mmlir --mlir-print-debuginfo --mlir-target=eravm %s | FileCheck %s

object "Test" {
  code {
    let a := mload(32)
    let b := mload(64)
    if lt(a, b) {
      mstore(0, 0)
    }
    if iszero(a) {
      mstore(1, 0)
    }
    return(0, 0)
  }
  object "Test_deployed" {
    code {
      let a := mload(32)
      let b := mload(64)
      if lt(a, b) {
        mstore(0, 0)
      }
      if iszero(a) {
        mstore(1, 0)
      }
      return(0, 0)
    }
  }
}
